<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.taobao.detailview"
    android:versionCode="2"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.taobao.detailview.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.taobao.detailview.Activity_ScrollView"
            android:label="@string/app_name" />
        <activity
            android:name="com.taobao.detailview.Activity_ScrollView_ListView"
            android:label="@string/app_name" />
        <activity
            android:name="com.taobao.detailview.Activity_ListViewWithFooter"
            android:label="@string/app_name" />
    </application>

</manifest>